{
    "provider": {
        "aws": [
            {
                "region": "us-west-2"
            }
        ]
    },
    "resource": {
        "aws_eks_cluster": {
            "example1": [
                {
                    "name": "example1",
                    "role_arn": "${aws_iam_role.example.arn}",
                    "vpc_config": [
                        {
                            "public_access_cidrs": [
                                "10.0.0.0/16"
                            ],
                            "subnet_ids": [
                                "subnet-12345"
                            ]
                        }
                    ]
                }
            ],
            "example2": [
                {
                    "name": "example2",
                    "role_arn": "${aws_iam_role.example_role.arn}",
                    "vpc_config": [
                        {
                            "endpoint_public_access": false,
                            "subnet_ids": [
                                "subnet-01"
                            ]
                        }
                    ]
                }
            ]
        },
        "aws_iam_role": {
            "example_role": [
                {
                    "assume_role_policy": "{\n  \"Version\": \"2012-10-17\",\n  \"Statement\": [\n    {\n      \"Action\": \"sts:AssumeRole\",\n      \"Effect\": \"Allow\",\n      \"Principal\": {\n        \"Service\": \"eks.amazonaws.com\"\n      }\n    }\n  ]\n}\n",
                    "name": "example_role"
                }
            ]
        }
    },
    "terraform": [
        {
            "required_providers": [
                {
                    "aws": {
                        "source": "hashicorp/aws",
                        "version": "~\u003e 4.16"
                    }
                }
            ],
            "required_version": "\u003e= 1.2.0"
        }
    ]
}